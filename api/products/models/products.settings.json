{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "products"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "version": {
      "type": "string"
    },
    "created_date": {
      "type": "datetime"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "ar_url": {
      "type": "string"
    },
    "buy_url": {
      "type": "string",
      "required": true
    }
  }
}
